	body.hold-transition.sidebar-mini.layout-fixed
		.wrapper
			// Navbar
			nav.main-header.navbar.navbar-expand.navbar-white.navbar-light
				// Left navbar links
				ul.navbar-nav
					li.nav-item
						a.nav-link(data-widget='pushmenu', href='#')
							i.fas.fa-bars
				// SEARCH FORM
				form.form-inline.ml-3
					.input-group.input-group-sm
						input.form-control.form-control-navbar(type='search', placeholder='Search', aria-label='Search')
						.input-group-append
							button.btn.btn-navbar(type='submit')
								i.fas.fa-search
				// Right navbar links
				ul.navbar-nav.ml-auto
					// Messages Dropdown Menu
					li.nav-item.dropdown
						a.nav-link(data-toggle='dropdown', href='#')
							i.far.fa-user
						//- span.badge.badge-danger.navbar-badge abc
						.dropdown-menu.dropdown-menu-lg.dropdown-menu-right
							
							a.dropdown-item.dropdown-footer(href='/users/logout') 
								i.far.fa-user(style="margin-right: 30px")
								span Logout
					
					
			// /.navbar

			// Main Sidebar Container
			aside.main-sidebar.sidebar-dark-primary.elevation-4
				// Brand Logo
				a.brand-link(href='index3.html')
					img.brand-image.img-circle.elevation-3(src='/admin/dist/img/AdminLTELogo.png', alt='AdminLTE Logo', style='opacity: .8')
					span.brand-text.font-weight-light Event App
				// Sidebar
				.sidebar
					// Sidebar user panel (optional)
					.user-panel.mt-3.pb-3.mb-3.d-flex
						.image
							img.img-circle.elevation-2(src='/admin/dist/img/user2-160x160.jpg', alt='User Image')
						.info
							a.d-block(href='#')=user.username
					// Sidebar Menu
					nav.mt-2
						ul.nav.nav-pills.nav-sidebar.flex-column(data-widget='treeview', role='menu', data-accordion='false')
							//
								Add icons to the links using the .nav-icon class
								with font-awesome or any other icon font library
							li.nav-item.has-treeview.menu-open
								a.nav-link.active(href='/admin')
									i.nav-icon.fas.fa-tachometer-alt
									p
										| Dashboard
								
							
							
							li.nav-item.has-treeview
								a.nav-link(href='#')
									i.nav-icon.far.fa-calendar-alt
									p
										| Events
										i.right.fas.fa-angle-left
								ul.nav.nav-treeview
									li.nav-item
										a.nav-link(href='/admin/event/add')
											i.far.fa-circle.nav-icon
											p Add Event
									li.nav-item
										a.nav-link(href='/admin/event/unpublished')
											i.far.fa-circle.nav-icon
											p List Unpublished
									li.nav-item
										a.nav-link(href='/admin/event/published')
											i.far.fa-circle.nav-icon
											p List Published

							
							li.nav-header LABELS
							li.nav-item
								a.nav-link(href='#')
									i.nav-icon.far.fa-circle.text-danger
									p.text Important
							li.nav-item
								a.nav-link(href='#')
									i.nav-icon.far.fa-circle.text-warning
									p Warning
							li.nav-item
								a.nav-link(href='#')
									i.nav-icon.far.fa-circle.text-info
									p Informational
					// /.sidebar-menu
					// /.sidebar

			// Content Wrapper. Contains page content
			.content-wrapper
			
